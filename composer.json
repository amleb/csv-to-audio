    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Adam Lebioda",
            "email": "adam@lebioda.co"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/http-foundation": "^4.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
